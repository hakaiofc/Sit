--STAMINA

local ReplicatedStorage = game:GetService("ReplicatedStorage")


local function task()
    local Event = game:GetService("ReplicatedStorage").Remotes.Situp
    Event:FireServer()
    local Event = game:GetService("ReplicatedStorage").Remotes.Situp
    Event:FireServer()
end


while true do

    for i = 1, 1 do
        task()
        wait()
    end
    wait(0.890)
end
